{
    "name": "hafael/autum-saml-sp",
    "description": "Autum SAML SP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": ["autum", "php", "laravel", "saml", "authentication", "webhook"],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.4",
        "litesaml/lightsaml": "^4.2",
        "spatie/laravel-webhook-client": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "authors": [
        {
            "name": "Rafael Villa Verde",
            "email": "rafael@autum.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Autum\\SAML\\": ["src", "test"]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autum\\SAML\\AutumSamlServiceProvider"
            ]
        }
    }
}
